{
  "name": "ps-docs",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "outputs": ["{workspaceRoot}/dist/apps/ps-docs"],
      "dependsOn": ["install"],
      "options": {
        "command": "npm run build && mkdir -p ../../dist/apps/ps-docs/.next && cp -r .next/* ../../dist/apps/ps-docs/.next/ ",
        "cwd": "apps/ps-docs"
      }
    },
    "serve": {
      "executor": "nx:run-commands",
      "options": { "command": "npm run dev", "cwd": "apps/ps-docs" }
    },
    "install": {
      "executor": "nx:run-commands",
      "options": { "command": "npm install", "cwd": "apps/ps-docs" },
      "cache": false
    }
  }
}
